package com.ibm.academia.apirest.services;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.beans.factory.annotation.Qualifier;

import com.ibm.academia.apirest.entities.Pabellon;
import com.ibm.academia.apirest.entities.Persona;
import com.ibm.academia.apirest.repositories.PabellonRepository;



public class PabellonDAOImpl implements PabellonDAO{
	
	@Autowired
	public PabellonDAOImpl(@Qualifier("repositorioPabellon")PabellonRepository repository) 
	{
		super(repository);
	}

	@Override
	public Iterable<Pabellon> FindPabellonByLocalidad(String localidad) 
	{
		return ((PabellonRepository)repository).FindPabellonByLocalidad(localidad);
	}


}
